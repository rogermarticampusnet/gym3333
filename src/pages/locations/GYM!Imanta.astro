                src="https://images.squarespace-cdn.com/content/v1/5f2729003294916c4921e58b/1667897046868-SWAF1Y3ED0NH7S7V2C4F/GymImanta-65.jpg?format=500w"
                class="rounded-lg"
            />
            <img
                src="https://images.squarespace-cdn.com/content/v1/5f2729003294916c4921e58b/1667897096812-UPWPFRECY6GYMZ7WLMNK/GymImanta-59.jpg?format=500w"
                class="rounded-lg"
            />
            <img
                src="https://images.squarespace-cdn.com/content/v1/5f2729003294916c4921e58b/1667897125034-KGI4ETV6WO4ZL16QJC2H/GymImanta-61.jpg?format=500w"
                class="rounded-lg"
            />
