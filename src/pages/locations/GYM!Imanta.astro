                src="https://images.squarespace-cdn.com/content/v1/5f2729003294916c4921e58b/1667897725496-WNY9L0T2O4D18AAKXFT1/GymImanta-42.jpg?format=500w"
                class="rounded-lg"
            />
            <img
                src="https://images.squarespace-cdn.com/content/v1/5f2729003294916c4921e58b/1667897784326-VR9TV1CCUVS4Q4M181YG/GymImanta-39.jpg?format=500w"
                class="rounded-lg"
            />
            <img
                src="https://images.squarespace-cdn.com/content/v1/5f2729003294916c4921e58b/1667897892799-ILQ5UK9NEC1I3PJX272U/GymImanta-29.jpg?format=500w"
